    <ItemGroup>
        <ProjectReference Include="..\Cake.AWS.Lambda\Cake.AWS.Lambda.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Cake.Core" Version="1.0.0" />
